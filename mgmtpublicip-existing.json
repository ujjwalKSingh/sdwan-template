{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string"
    },
    "MgmtpublicIPAddressName": {
      "type": "string"
    },
    "MgmtpublicIPAddressType": {
      "type": "string"
    },
    "MgmtdnsPrefix": {
      "type": "string"
    },
    "MgmtpublicIpRGName": {
      "type": "string"
    },
    "MgmtpublicIPNewOrExisting" : {
      "type" : "string"
    }
  },
  "variables": {
    "publicIpId": "[resourceId(parameters('MgmtpublicIpRGName'),concat('Microsoft.Network','/','publicIPAddresses'),parameters('MgmtpublicIPAddressName'))]"
  },
  "resources": [],
  "outputs": {
    "publicIpId": {
      "value": "[variables('publicIpId')]",
      "type": "string"
    },
    "fqdn": {
      "value": "[reference(variables('publicIpId'),'2015-06-15').dnsSettings.fqdn]",
      "type": "string"
    }
  }
}
